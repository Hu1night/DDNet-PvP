# fng room config

map AliveFNG
scorelimit 200
timelimit 10
kill_delay 20

# room votes
add_vote " " "info"
add_vote "=== 房间设置 ===" "info"
add_vote "Restart" "restart 10"

# rulesets
add_vote "Rule: 10min 200points" "scorelimit 200;timelimit 10"
add_vote "Rule: 20min 500points" "scorelimit 500;timelimit 20"
add_vote "Rule: 30min 700points" "scorelimit 700;timelimit 30"
exec "room_config/modes/fngsuperconfig.rcfg"

# add mode switch votes
exec "room_config/modes.rcfg"
